#include <iostream>
#include <string>
using namespace std;
class Car {
public:
string brand;
string model;
int year;
};
int main() {
// Create an object of Car
Car carObj1;
carObj1.brand = "Koenigsegg";
carObj1.model = "Regera";
carObj1.year = 2015;
// Create another object of Car
Car carObj2;
carObj2.brand = "Porsche";
carObj2.model = "911 GT3 RS";
carObj2.year = 2003;
// Print attribute values
cout << carObj1.brand << " " << carObj1.model << " " << carObj1.year << "\n";
cout << carObj2.brand << " " << carObj2.model << " " << carObj2.year << "\n";
return 0;
}